    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug Consumer with SQS properties",
            "request": "launch",
            "mainClass": "org.ipan.awspractice.sqs.consumer.ConsumerApplication",
            "projectName": "consumer",
            "vmArgs": [
                "-Dsqs.queue.name=basic-q",
                "-Dsqs.region=us-east-1"
            ]
        },
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "org.ipan.Main",
            "projectName": "rdstest"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "org.ipan.App",
            "projectName": "rdstest"
        }
    ]
}